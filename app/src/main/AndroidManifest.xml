<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zosimadis.ilias.materialdesign">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:name=".com.zosimadis.ilias.materialdesign.MyApplication"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".com.zosimadis.ilias.materialdesign.MainActivity"
            android:label="@string/title_activity_main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".com.zosimadis.ilias.materialdesign.NextActivity"
            android:label="@string/title_activity_next">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".com.zosimadis.ilias.materialdesign.MainActivity" />
        </activity>
    </application>

</manifest>
